@model List<ResultLast5CarWithBrandDto>

<div class="col-xl-6">
	<div class="card">
		<div class="card-body">
			<h4 class="card-title">Araç Bilgileri</h4>
			<div class="table-responsive">
				<table class="table table-centered table-hover table-xl mb-0" id="recent-orders">
					<thead>
						<tr>
							<th class="border-top-0">Marka</th>
							<th class="border-top-0">Model</th>
							<th class="border-top-0">Km</th>
							<th class="border-top-0">Vites Türü</th>
							<th class="border-top-0">Yakıt</th>
						</tr>
					</thead>
					<tbody>
						@foreach (var item in Model)
						{
							<tr>
								<td class="text-truncate">@item.BrandName</td>
								<td class="text-truncate">@item.Model</td>
								<td>
									<span class="badge badge-soft-secondary p-2">@item.Km</span>
								</td>
								<td class="text-truncate">@item.Transmission</td>
								<td class="text-truncate">@item.Fuel</td>
							</tr>
						}
					</tbody>
				</table>
			</div>
		</div> 
	</div> 
</div> 